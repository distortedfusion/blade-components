{
    "name": "distortedfusion/laravel-tailwind-layout",
    "keywords": ["laravel", "tailwind", "layout", "distortedfusion"],
    "authors": [
        {
            "name": "Kevin Dierkx",
            "email": "kevin@distortedfusion.com"
        }
    ],
    "require": {
        "php": "^7.4|^8.0",
        "illuminate/support": "^7.0|^8.0",
        "protonemedia/laravel-form-components": "^2.5"
    },
    "autoload": {
        "psr-4": {
            "DistortedFusion\\Tailwind\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "DistortedFusion\\Tailwind\\LayoutServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
